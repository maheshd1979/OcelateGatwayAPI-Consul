{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/token",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5202
        }
      ],
      "ServiceName": "AuthService",
      "UpstreamPathTemplate": "/auth",
      "UpstreamHttpMethod": [ "POST", "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/products/list",
      "DownstreamScheme": "http",
      "ServiceName": "ProductService",
      "UpstreamPathTemplate": "/products",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 3,
        "QuotaExceededMessage": "Rate limit exceeded, please try again later.",
        "HttpStatusCode": 429,
        "DisableRateLimitHeaders": false
      },
      "FileCacheOptions": {
        "TtlSeconds": 30,
        "Region": "ProductsCache"
      }
    }
  ],
  "GlobalConfiguration": {
   // "BaseUrl": "https://localhost:7099",
    "BaseUrl": "http://localhost:5132",
    "ServiceDiscoveryProvider": {
      "Type": "Consul",
      "Scheme": "http",
      "Host": "localhost",
      "Port": 8500
    }
  }
}
